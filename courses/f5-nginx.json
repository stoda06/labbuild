{
    "f5-nginx": {
        "vendor": "f5",
        "version": "nginx",
        "class": "f5-class{Y}",
        "group": [
            {
                "name": "app-protect",
                "component": [
                    {
                        "component_name": "app-protect",
                        "base_vm": "f5-nginx-plus-app-protect",
                        "clone_vm": "f5-nginx-plus-app-protect-{X}"
                    }
                ]
            },
            {
                "name": "jump-box",
                "component": [
                    {
                        "component_name": "jump-box",
                        "base_vm": "f5-nginx-jump-box",
                        "clone_vm": "f5-nginx-jump-box-{X}"
                    }
                ]
            },
            {
                "name": "srv",
                "component": [
                    {
                        "component_name": "nginx-juice-shop",
                        "base_vm": "f5-nginx-juice-shop",
                        "clone_vm": "f5-nginx-juice-shop-{Y}"
                    },
                    {
                        "component_name": "nginx-mgmt-suite",
                        "base_vm": "f5-nginx-mgmt-suite",
                        "clone_vm": "nginx-mgmt-suite-{Y}"
                    }
                ]
            },
            {
                "name": "nginx-ingress-ctrl",
                "component": [
                    {
                        "component_name": "nginx-ingress-ctrl",
                        "base_vm": "f5-nginx-ingress-ctrl",
                        "clone_vm": "f5-nginx-ingress-ctrl-{X}"
                    }
                ]
            },
            {
                "name": "nginx-open-source",
                "component": [
                    {
                        "component_name": "nginx-open-source",
                        "base_vm": "nginx-open-source",
                        "clone_vm": "nginx-open-source-{X}"
                    }
                ]
            }
        ],
        "network": [
            {
                "switch": "vs",
                "port_groups": [
                    {
                        "port_group_name": "f5-nginx-int",
                        "vlan_id": 111
                    }
                ]
            }
        ]
    }
}